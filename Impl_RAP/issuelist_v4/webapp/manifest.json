{
  "_version": "1.59.0",
  "sap.app": {
    "id": "issuelistv4",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.12.5",
      "toolsId": "1a523ce8-1340-45cd-89fe-ea6fee9ed46a"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata4/sap/zui_dt_inv_h_02_o4/srvd/sap/zui_dt_inv_h_02_o4/0001/",
        "type": "OData",
        "settings": {
          "annotations": [
            "annotation"
          ],
          "localUri": "localService/metadata.xml",
          "odataVersion": "4.0"
        }
      },
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.140.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ushell": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "issuelistv4.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "IssueList",
          "target": "IssueList"
        },
        {
          "pattern": "Issue({key}):?query:",
          "name": "IssueObjectPage",
          "target": "IssueObjectPage"
        }
      ],
      "targets": {
        "IssueList": {
          "type": "Component",
          "id": "IssueList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/Issue",
              "variantManagement": "Page",
              "navigation": {
                "Issue": {
                  "detail": {
                    "route": "IssueObjectPage"
                  }
                }
              },
              "initialLoad": "Enabled",
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "selectionMode": "Single",
                    "enableExport": false
                  }
                }
              }
            }
          }
        },
        "IssueObjectPage": {
          "type": "Component",
          "id": "IssueObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": false,
              "contextPath": "/Issue",
              "content": {
                "header": {
                  "actions": {
                    "DataFieldForAction::com.sap.gateway.srvd.zui_dt_inv_h_02_o4.v0001.checkPassword::com.sap.gateway.srvd.zui_dt_inv_h_02_o4.v0001.IssueType": {
                      "afterExecution": {
                        "navigateToInstance": true
                      }
                    },
                    "Password": {
                      "press": "issuelistv4.ext.controller.Button.Button",
                      "visible": true,
                      "enabled": true,
                      "text": "Unlock",
                      "position": {
                        "placement": "After",
                        "anchor": "DeleteAction"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  }
}
