<mvc:View controllerName="maps.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:vbm="sap.ui.vbm" 
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <vbm:AnalyticMap width="100%" id="_IDGenAnalyticMap1" regions="{viewModel>/mapData}" >
                <vbm:regions>
                    <vbm:Region id="_IDGenRegion1"  code="{viewModel>Text}" color="{viewModel>color}">

                    </vbm:Region>
                </vbm:regions>
            </vbm:AnalyticMap>
        </content>
    </Page>
</mvc:View>
